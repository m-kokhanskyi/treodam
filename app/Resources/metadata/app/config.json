                        },
                        "public": {
                            "min": 100,
                            "max": 100000000
                        }
                    },
                    "quality": {
                        "min": 10,
                        "max": 100
                    },
                    "colorDepth": [
                        8
                    ],
                    "colorSpace": [
                        "RGB",
                        "SRGB"
                    ],
                    "scale": {
                        "min": {
                            "width": 50000,
                            "height": 50000
                        }
                    },
                    "unique": {}
                }
            },
