                        "pdf"
                    ],
                    "unique": {
                        "skip": true
                    }
                },
                "variations": {
                    "web": {}
                }
            },
            "image": {
                "nature": "image",
                "validations": {
                    "unique": {
                        "skip": true
                    },
                    "extension": [
                        "png",
                        "jpg",
                        "jpeg"
                    ]
                },
                "variations": {
                    "200x200": {},
                    "low-quality": {}
                }
            },
            "file": {
                "nature": "file",
                "validations": {
                    "size": {
                        "private": 1,
                        "public": 2
                    },
                    "unique": {
                        "skip": true
                    },
                    "extension": [
                        "doc",
                        "docx",
                        "html"
                    ]
                }
            }
        }
    },
    "variations": {
        "low-quality": {
            "auto": true,
            "private": false,
            "handlers": {
                "quality": {
                    "params": {
                        "quality": 25
                    }
                }
            },
            "validations": {
                "size": 1
            }
        },
        "200x200": {
            "auto": true,
            "handlers": {
                "resize": {
                    "params": {
                        "width": 200,
                        "height": 200
                    }
                },
                "quality": {
                    "params": {
                        "quality": 10
                    }
                }
            },
            "validations": {
                "dimension": {
                    "minWidth": 200,
                    "minHeight": 200
                }
            }
        },
        "web": {
            "auto": false
        }
    }
