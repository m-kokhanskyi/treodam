{
    "types": {
        "global": {
            "nature": "file",
            "validations": {
                "size": 5,
                "unique": {}
            }
        },
        "default": {
            "validations": {
                "size": 1
            }
        },
        "custom": {
            "icon": {
                "nature": "image",
                "validations": {
                    "mime": {
                        "pattern": "image",
                        "message": "Only images"
                    },
                    "ratio": 1
                }
            },
            "gallery-image": {
                "nature": "image",
                "validations": {
                    "mime": {
                        "pattern": "image",
                        "message": "Only images"
                    },
                    "size": {
                        "private": {
                            "min": 100,
                            "max": 100000000
                        },
                        "public": {
                            "min": 100,
                            "max": 100000000
                        }
                    },
                    "quality": {
                        "min": 10,
                        "max": 100
                    },
                    "colorDepth": [
                        8
                    ],
                    "colorSpace": [
                        "RGB",
                        "SRGB"
                    ],
                    "scale": {
                        "min": {
                            "width": 50000,
                            "height": 50000
                        }
                    },
                    "unique": {}
                }
            },
            "description-image": {
                "nature": "image",
                "validations": {
                    "mime": {
                        "pattern": "image",
