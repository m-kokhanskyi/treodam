{
  "fields": {
    "height": {
      "type": "int",
      "required": false,
      "readOnly": true,
      "disableFormatting": false,
      "audited": false,
      "tooltip": false
    },
    "width": {
      "type": "int",
      "required": false,
      "readOnly": true,
      "disableFormatting": false,
      "audited": false,
      "tooltip": false
    },
    "colorSpace": {
      "type": "varchar",
      "required": false,
      "trim": true,
      "readOnly": true,
      "audited": false,
      "tooltip": false
    },
    "colorDepth": {
      "type": "varchar",
      "required": false,
      "trim": true,
      "readOnly": true,
      "audited": false,
      "tooltip": false
    },
    "orientation": {
      "type": "varchar",
      "required": false,
      "trim": true,
      "readOnly": true,
      "audited": false,
      "tooltip": false
    },
    "isActive": {
      "notNull": true,
      "type": "bool",
      "default": true,
      "audited": false,
      "readOnly": false,
      "tooltip": false
    },
    "tags": {
      "type": "multiEnum",
      "storeArrayValues": true,
      "required": false,
      "fontSize": 1,
      "options": [
        "tag1",
        "tag2",
        "tag3",
        "tag4",
        "tag5"
      ],
      "optionColors": {
        "tag1": "FF0000",
        "tag2": "FFFF00",
        "tag3": "00FF00",
        "tag4": "0000FF",
        "tag5": "00FFFF"
      },
      "audited": false,
      "readOnly": false
    },
    "renditions": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "noLoad": true,
      "importDisabled": true
    },
    "collection": {
      "type": "link"
    },
    "assetMetaDatas": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "noLoad": true,
      "importDisabled": true
    },
    "assetVersions": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "noLoad": true,
      "importDisabled": true
    },
    "path": {
      "type": "varchar",
      "emHidden": true,
      "hidden": true,
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "layoutListDisabled": true
    },
    "assetCategories": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true,
      "noLoad": true
    },
    "name": {
      "type": "varchar",
      "required": true,
      "trim": true,
      "isMultilang": true,
      "view": "dam:views/fields/name-from-file"
    },
    "nameOfFile": {
      "type": "varchar",
      "required": true,
      "trim": true
    },
    "description": {
      "type": "text",
      "rows": 4,
      "lengthOfCut": 400,
      "isMultilang": true
    },
    "metaData": {
      "type": "text"
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "assignedUser": {
      "type": "link",
      "required": true,
      "view": "views/fields/assigned-user"
    },
    "teams": {
      "type": "linkMultiple",
      "view": "views/fields/teams"
    },
    "private": {
      "notNull": true,
      "type": "bool",
      "default": false,
      "audited": false,
      "readOnly": false,
      "tooltip": false
    },
    "code": {
      "type": "varchar",
      "layoutMassUpdateDisabled": true,
      "required": true,
      "trim": true,
      "emHidden": true,
      "view": "dam:views/fields/code-from-name"
    },
    "type": {
      "type": "enum",
      "required": false,
      "fontSize": 1,
      "options": [
        "Gallery Image",
        "Description Image",
        "Icon",
        "PDF",
        "Office Document",
        "Text",
        "CSV",
        "PDF Document",
        "Archive"
      ],
      "optionColors": {
        "File": "fff",
        "PDF": "fff",
        "Image": "fff",
        "Icon": "fff",
        "Description Image": "fff",
        "Gallery Image": "fff",
        "Office Document": "fff",
        "Text": "fff",
        "CSV": "fff",
        "PDF Document": "fff",
        "Archive": "fff"
      },
      "default": "Gallery Image",
      "isSorted": false,
      "audited": false,
      "readOnly": false,
      "tooltip": false
    },
    "image": {
      "type": "image",
      "required": true,
      "previewSize": "small",
      "audited": false,
      "tooltip": false,
      "layoutMassUpdateDisabled": true,
      "view": "dam:views/fields/image"
    },
    "file": {
      "type": "file",
      "required": true,
      "sourceList": [],
      "audited": false,
      "tooltip": false,
      "view": "dam:views/fields/file",
      "layoutMassUpdateDisabled": true
    },
    "size": {
      "type": "unit",
      "required": false,
      "measure": "File Size",
      "audited": false,
      "readOnly": true,
      "tooltip": false
    }
  },
  "links": {
    "renditions": {
      "type": "hasMany",
      "foreign": "asset",
      "entity": "Rendition",
      "audited": false
    },
    "collection": {
      "type": "belongsTo",
      "foreign": "assets",
      "entity": "Collection",
      "audited": false
    },
    "assetMetaDatas": {
      "type": "hasMany",
      "foreign": "asset",
      "entity": "AssetMetaData",
      "audited": false
    },
    "assetVersions": {
      "type": "hasMany",
      "foreign": "asset",
      "entity": "AssetVersion",
      "audited": false
    },
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "assignedUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "teams": {
      "type": "hasMany",
      "entity": "Team",
      "relationName": "EntityTeam",
      "layoutRelationshipsDisabled": true
    },
    "image": {
      "type": "belongsTo",
      "entity": "Attachment",
      "skipOrmDefs": true
    },
    "file": {
      "type": "belongsTo",
      "entity": "Attachment",
      "skipOrmDefs": true
    },
    "assetCategories": {
      "type": "hasMany",
      "relationName": "assetCategoryAsset",
      "foreign": "assets",
      "entity": "AssetCategory",
      "audited": false
    }
  },
  "collection": {
    "sortBy": "createdAt",
    "asc": false
  },
  "indexes": {
    "name": {
      "columns": [
        "name",
        "deleted"
      ]
    },
    "type": {
      "columns": [
        "type"
      ]
    },
    "assignedUser": {
      "columns": [
        "assignedUserId",
        "deleted"
      ]
    }
  }
}
