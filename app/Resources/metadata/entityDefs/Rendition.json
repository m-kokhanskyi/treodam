{
  "fields"    : {
    "name"             : {
      "type"    : "varchar",
      "required": true,
      "trim"    : true
    },
    "path"             : {
      "type"                    : "varchar",
      "emHidden"                : true,
      "hidden"                  : true,
      "layoutDetailDisabled"    : true,
      "layoutMassUpdateDisabled": true,
      "layoutListDisabled"      : true
    },
    "description"      : {
      "type": "text"
    },
    "createdAt"        : {
      "type"    : "datetime",
      "readOnly": true
    },
    "modifiedAt"       : {
      "type"    : "datetime",
      "readOnly": true
    },
    "createdBy"        : {
      "type"    : "link",
      "readOnly": true,
      "view"    : "views/fields/user"
    },
    "modifiedBy"       : {
      "type"    : "link",
      "readOnly": true,
      "view"    : "views/fields/user"
    },
    "assignedUser"     : {
      "type"    : "link",
      "required": true,
      "view"    : "views/fields/assigned-user"
    },
    "teams"            : {
      "type": "linkMultiple",
      "view": "views/fields/teams"
    },
    "type"             : {
      "type"    : "enum",
      "required": false,
      "options" : [
        "wlq",
        "whq",
        "githumb1",
        "dithumb1",
        "githumb2",
        "dithumb2",
        "githumb3",
        "dithumb3",
        "githumb4",
        "dithumb4",
        "watermark",
        "test"
      ],
      "default" : "wlq",
      "isSorted": false,
      "audited" : false,
      "readOnly": false,
      "tooltip" : false,
      "view"    : "dam:views/rendition/fields/type"
    },
    "file"             : {
      "type"                    : "file",
      "required"                : true,
      "sourceList"              : [],
      "audited"                 : false,
      "tooltip"                 : false,
      "view"                    : "dam:views/rendition/fields/file",
      "layoutMassUpdateDisabled": true
    },
    "private"          : {
      "notNull" : true,
      "type"    : "bool",
      "default" : false,
      "audited" : false,
      "readOnly": false,
      "tooltip" : false
    },
    "width"            : {
      "type"             : "int",
      "required"         : false,
      "disableFormatting": false,
      "audited"          : false,
      "readOnly"         : false,
      "tooltip"          : false
    },
    "height"           : {
      "type"             : "int",
      "required"         : false,
      "disableFormatting": false,
      "audited"          : false,
      "readOnly"         : false,
      "tooltip"          : false
    },
    "size"             : {
      "type"    : "unit",
      "required": false,
      "measure" : "File Size",
      "audited" : false,
      "readOnly": false,
      "tooltip" : false
    },
    "colorDepth"       : {
      "type"    : "varchar",
      "required": false,
      "trim"    : true,
      "audited" : false,
      "readOnly": false,
      "tooltip" : false
    },
    "colorSpace"       : {
      "type"    : "varchar",
      "required": false,
      "trim"    : true,
      "audited" : false,
      "readOnly": false,
      "tooltip" : false
    },
    "orientation"      : {
      "type"    : "varchar",
      "required": false,
      "trim"    : true,
      "audited" : false,
      "readOnly": false,
      "tooltip" : false
    },
    "asset"            : {
      "type": "link"
    },
    "assetMetaDatas"   : {
      "type"                    : "linkMultiple",
      "layoutDetailDisabled"    : true,
      "layoutMassUpdateDisabled": true,
      "noLoad"                  : true,
      "importDisabled"          : true
    },
    "nameOfFile"       : {
      "type": "varchar",
      "trim": true,
      "view": "dam:views/rendition/fields/name-of-file"
    },
    "renditionVersions": {
      "type"                    : "linkMultiple",
      "layoutDetailDisabled"    : true,
      "layoutMassUpdateDisabled": true,
      "noLoad"                  : true,
      "importDisabled"          : true
    }
  },
  "links"     : {
    "createdBy"        : {
      "type"  : "belongsTo",
      "entity": "User"
    },
    "modifiedBy"       : {
      "type"  : "belongsTo",
      "entity": "User"
    },
    "assignedUser"     : {
      "type"  : "belongsTo",
      "entity": "User"
    },
    "teams"            : {
      "type"                       : "hasMany",
      "entity"                     : "Team",
      "relationName"               : "EntityTeam",
      "layoutRelationshipsDisabled": true
    },
    "file"             : {
      "type"       : "belongsTo",
      "entity"     : "Attachment",
      "skipOrmDefs": true
    },
    "asset"            : {
      "type"   : "belongsTo",
      "foreign": "renditions",
      "entity" : "Asset",
      "audited": false
    },
    "assetMetaDatas"   : {
      "type"   : "hasMany",
      "foreign": "rendition",
      "entity" : "AssetMetaData",
      "audited": false
    },
    "renditionVersions": {
      "type"   : "hasMany",
      "foreign": "rendition",
      "entity" : "RenditionVersion",
      "audited": false
    }
  },
  "collection": {
    "sortBy": "createdAt",
    "asc"   : false
  },
  "indexes"   : {
    "name"        : {
      "columns": [
        "name",
        "deleted"
      ]
    },
    "assignedUser": {
      "columns": [
        "assignedUserId",
        "deleted"
      ]
    }
  }
}